{
  "name": "@homebridge/node-pty-prebuilt-multiarch",
  "description": "Fork pseudoterminals in Node.JS",
  "author": {
    "name": "Microsoft Corporation"
  },
  "version": "0.11.1",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./typings/node-pty.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/homebridge/node-pty-prebuilt-multiarch.git"
  },
  "files": [
    "binding.gyp",
    "lib/",
    "scripts/",
    "src/",
    "deps/",
    "typings/",
    "prebuilds/darwin-x64",
    "prebuilds/darwin-arm64",
    "prebuilds/linux-x64",
    "prebuilds/linux-ia32",
    "prebuilds/linux-arm",
    "prebuilds/linux-arm64",
    "prebuilds/win32-x64",
    "prebuilds/win32-ia32"
  ],
  "homepage": "https://github.com/homebridge/node-pty-prebuilt-multiarch",
  "bugs": {
    "url": "https://github.com/homebridge/node-pty-prebuilt-multiarch/issues"
  },
  "keywords": [
    "pty",
    "tty",
    "terminal",
    "pseudoterminal",
    "forkpty",
    "openpty"
  ],
  "scripts": {
    "build": "tsc -b ./src/tsconfig.json",
    "watch": "tsc -b -w ./src/tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src/",
    "install": "node-gyp-build",
    "postinstall": "node scripts/post-install.js",
    "test": "cross-env NODE_ENV=test mocha -R spec --exit lib/*.test.js",
    "posttest": "npm run lint",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "nan": "^2.18.0",
    "node-gyp": "^9.4.0",
    "node-gyp-build": "^4.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^18.18.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "mocha": "^10.2.0",
    "node-abi": "^3.47.0",
    "prebuildify": "^5.0.1",
    "ps-list": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
